# Think Graph 环境配置示例文件
# 复制此文件为 .env.local 并填写相应的值

# OpenAI API配置
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4-turbo-preview
OPENAI_TEMPERATURE=0.7
OPENAI_MAX_TOKENS=4000

# Supabase配置
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# 应用配置
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=Think Graph
NEXT_PUBLIC_APP_DESCRIPTION=AI-Powered Knowledge Graph Tool

# 认证配置
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret

# 搜索配置
NEXT_PUBLIC_MAX_SEARCH_RESULTS=50
NEXT_PUBLIC_SEARCH_CACHE_TIME=3600

# 图谱配置
NEXT_PUBLIC_DEFAULT_GRAPH_LAYOUT=force
NEXT_PUBLIC_MAX_NODES_PER_GRAPH=100
NEXT_PUBLIC_NODE_LIMIT_FREE_TIER=50

# 缓存配置
REDIS_URL=your_redis_url
CACHE_TTL=3600

# 监控配置
NEXT_PUBLIC_ANALYTICS_ID=your_analytics_id
SENTRY_DSN=your_sentry_dsn

# 第三方服务集成
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret
DISCORD_CLIENT_ID=your_discord_client_id
DISCORD_CLIENT_SECRET=your_discord_client_secret 